            <ToggleGroup fx:id="langtoggler" />
         </toggleGroup></ToggleButton>
       <ToggleButton fx:id="en" layoutX="14.0" layoutY="80.0" prefHeight="60.0" prefWidth="100.0" text="English" toggleGroup="$langtoggler" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="236.0" />
